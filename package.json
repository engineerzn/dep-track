{
  "name": "dep-track",
  "version": "0.0.1",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "dev": "node dist/index.js",
    "build": "yarn build:cjs & yarn build:esm",
    "build:cjs": "tsc --outDir ./dist/cjs --p ./config/cjs/tsconfig.json && cp -R public dist/cjs",
    "build:esm": "tsc --outDir ./dist/esm --p ./config/esm/tsconfig.json && cp -R public dist/esm"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "express": "^4.21.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "typescript": "^5.6.3"
  },
  "author": {
    "name": "zn"
  },
  "license": "MIT",
  "description": "Dependency Graph",
  "keywords": []
}
