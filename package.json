{
  "name": "dep-track",
  "version": "0.0.1",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    }
  },
  "scripts": {
    "dev": "node test.js",
    "build": "yarn build:cjs & yarn build:esm",
    "build:cjs": "tsc --outDir ./module/cjs --p ./cjs/tsconfig.json",
    "build:esm": "tsc --outDir ./module/esm --p ./esm/tsconfig.json",
    "build:orig": "tsc --outDir ./dist && cp -R src/public dist"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "express": "^4.21.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "d3": "^7.9.0"
  }
}
